#include <dt-bindings/zmk/matrix_transform.h>

/ {
    chosen {
        zmk,kscan = &kscan0;
        zmk,matrix_transform = &default_transform;
    };

    vbatt: vbatt {
	compatible = "zmk,battery-nrf-vddh";
    };

    default_transform: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
        columns = <6>;
        rows = <5>;     
        map = <
        RC(0,1) RC(0,2) RC(0,3) RC(0,4)
        RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(0,5)
RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(1,5) RC(4,3)
        RC(3,1) RC(3,2) RC(3,3) RC(3,4) RC(2,5)
                                RC(4,0) RC(4,1) RC(4,2)                  
   


        >;
    };

    kscan0: kscan_0 {
        compatible = "zmk,kscan-gpio-matrix";

        diode-direction = "col2row";
        row-gpios
            = <&gpio1 12 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> //r1-7 1.12 1.13 0.02 0.03 0.28** 1.11** 0.10*thumbs
            , <&gpio1 13 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> 
            , <&gpio0 2 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> 
            , <&gpio0 3 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> 
//            , <&gpio0 28 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
//            , <&gpio1 11 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&gpio0 10 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            ;

    };
};
